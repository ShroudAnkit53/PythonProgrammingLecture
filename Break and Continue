# 'break' statement :-
for i in range(12):
    if(i == 10):  # at i=10, it will break
        break
    print(5,"x",i+1,"=",5*(i+1))

print("Just exit the loop")

# 'continue' statement :-
for i in range(12):
    if(i == 10):
        print("Skip the iteration")
        continue
    print(5,"x",i,"=",5*i)
